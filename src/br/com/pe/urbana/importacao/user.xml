					<Field name="sexo" type="STRING" length="1" />
					<Field name="dataNascimento" type="STRING" length="8" />
					<Field name="nacionalidade" type="STRING" length="3" />
					<Field name="estadoCivil" type="STRING" length="2" />
					<Field name="nomeMae" type="STRING" length="100" />
					<Field name="nomePai" type="STRING" length="100" />
					<Field name="caminhoFoto" type="STRING" length="100" />
					<Field name="obs" type="STRING" length="512" />
				</GroupOfFields>
			</Record>
		</GroupOfRecords>			
	</FlatFile>

</txg:Texgit>